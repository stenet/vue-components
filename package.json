{
  "name": "stef-vue-components",
  "version": "0.0.13",
  "module": "./dist/vue-components.es.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/vue-components.es.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css"
    }
  },
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && vue-tsc --emitDeclarationOnly",
    "git": "vue-tsc --noEmit && vite build --config=vite-git.config.ts",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@vueuse/core": "^8.2.6",
    "devextreme": "^21.2.7",
    "devextreme-vue": "^21.2.7",
    "moment": "^2.29.x",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "peerDependencies": {
    "@fortawesome/fontawesome-free": "^6.x",
    "@vueuse/core": "^8.2.6",
    "devextreme": "*",
    "devextreme-vue": "*",
    "moment": "^2.29.3",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.26",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "less": "^4.1.2",
    "postcss": "^8.4.12",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vite": "^2.9.1",
    "vue-tsc": "^0.34.7"
  }
}
